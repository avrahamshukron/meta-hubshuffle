[connection]
id=Hotspot
type=wifi
permissions=
secondaries=
autoconnect=true

[wifi]
hidden=true
mac-address-blacklist=
mode=ap
seen-bssids=
ssid=@@OPENMENTOS_WIFI_AP_SSID@@

[wifi-security]
group=ccmp;
key-mgmt=wpa-psk
pairwise=ccmp;
proto=rsn;
psk=@@OPENMENTOS_WIFI_AP_PSK@@

[ipv4]
dns-search=
method=shared
address1=192.168.1.1/24

[ipv6]
dns-search=
method=auto
